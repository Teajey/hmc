<myPage xmlns:c="https://github.com/Teajey/hyprctl/blob/v0.3.0/README.md">
  <Title>Login to my thing</Title>
  <c:Form Method="POST">
    <login>
      <c:Input Label="Username" Name="username" Value="" Required="true"></c:Input>
      <c:Input Label="Password" Name="password" Type="password" Value="" Required="true"></c:Input>
      <c:Input Label="Confirm password" Name="confirmPassword" Type="password" Value="" Required="true"></c:Input>
      <c:Select Label="Favourite food" Name="favFood">
        <c:Option>fruit</c:Option>
        <c:Option>vegetables</c:Option>
        <c:Option>meat</c:Option>
        <c:Option>fish</c:Option>
        <c:Option>bugs</c:Option>
      </c:Select>
      <c:Map Label="Any other arbitrary information you wanna provide?" Name="misc"></c:Map>
      <c:Link Href="/register">Register</c:Link>
      <c:Submit>Login</c:Submit>
    </login>
  </c:Form>
</myPage>
